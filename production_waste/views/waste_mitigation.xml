<odoo>

    <data>
    
        <record id="waste_mitigation_view_tree" model="ir.ui.view">
            <field name="name">waste.mitigation.view.tree</field>
            <field name="model">waste.mitigation</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="waste_mitigation_action" model="ir.actions.act_window">
            <field name="name">Waste Mitigation</field>
            <field name="res_model">waste.mitigation</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
    
    </data>
    

</odoo>
