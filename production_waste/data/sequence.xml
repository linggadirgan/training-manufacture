<odoo>

    <data noupdate="1">
        <record id="seq_waste_record" model="ir.sequence">
            <field name="name">Waste Record Sequence</field>
            <field name="code">waste.record</field>
            <field name="prefix">WR/%(year)s/%(month)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

</odoo>
